import time

def timer(func, key=5):
    def wrapper(*args, **kwargs):
        start = time.time()
        func()
        total = time.time() - start
        print('Time:', total)
        #return rv
    return wrapper

@timer
def test():
    for _ in range(100000):
        pass

@timer
def test2():
    for _ in range(1000000):
        pass

test()
test2()